    "charset": "utf8",
    "dialectOptions": {
      "collate": "utf8_general_ci"
    },
    "timestamps": true,
    "createdAt": "date_created",
    "updatedAt": "date_modified",
    "createdBy": "created_by",
    "updatedBy": "updated_by"
  }
